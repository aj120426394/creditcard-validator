{
  "private": true,
  "main": "./app/index.jsx",
  "scripts": {
    "start": "node server.js",
    "dev": "yarn development",
    "development": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --colors",
    "build": "yarn production",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test:unit:js": "jest",
    "lint:js": "eslint ./app-js/**/*.{js,jsx}",
    "lint:scss": "stylelint ./app-js/**/*.jsx",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy": "cp -r /app/source/multi-function-site/dist/ /app/public/mfs",
    "clean": "rm -r /app/public/mfs"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/jest/*.js"
    ],
    "testMatch": [
      "**/test-unit-js/**/*Test.js",
      "**/app-js/**/*.test.(jsx|js)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/fileMock.js",
      "\\.(css|less)$": "<rootDir>/jest/styleMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
